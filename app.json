{
  "expo": {
    "name": "FaceIdApp3",
    "slug": "faceidapp3",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/common/styles/img/icon.png",
    "splash": {
      "image": "./src/common/styles/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.checker.faceid",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The application requires access to your photos to allow you to upload a profile picture."
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": ""
  }
}
